name: dockercompose561920710202547078
services:
  searchapi:
    build:
      context: C:\Users\user\Documents\Dev\OigaTech
      dockerfile: SearchApi/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: searchapi
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 50001
      published: "52000"
      protocol: tcp
  searchapi-dapr:
    command:
    - ./daprd
    - -app-id
    - SearchApi
    - -app-port
    - "80"
    depends_on:
      searchapi:
        condition: service_started
    image: daprio/daprd:latest
    network_mode: service:searchapi
  userweb:
    build:
      context: C:\Users\user\Documents\Dev\OigaTech
      dockerfile: UserApi/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: userweb
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 443
      protocol: tcp
    - mode: ingress
      target: 50001
      published: "51001"
      protocol: tcp
    volumes:
    - type: bind
      source: C:\Users\user\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
  userweb-dapr:
    command:
    - ./daprd
    - -app-id
    - UserApi
    - -app-port
    - "80"
    depends_on:
      userweb:
        condition: service_started
    image: daprio/daprd:latest
    network_mode: service:userweb
networks:
  default:
    name: dockercompose561920710202547078_default